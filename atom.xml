---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <link href="https://thenewcovenantofgod.github.io/atom.xml" rel="self"/>
 <link href="https://thenewcovenantofgod.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://thenewcovenantofgod.github.io/</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://thenewcovenantofgod.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://thenewcovenantofgod.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
